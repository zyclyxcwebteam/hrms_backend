{
  "kind": "collectionType",
  "collectionName": "attendances",
  "info": {
    "name": "Attendance"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Date": {
      "type": "date"
    },
    "Emp_Name": {
      "type": "string"
    },
    "Action": {
      "type": "string"
    },
    "Time": {
      "type": "string"
    },
    "Status": {
      "type": "string"
    },
    "Notes": {
      "type": "string"
    },
    "Seconds": {
      "type": "biginteger"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "attendances"
    }
  }
}
